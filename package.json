{
	"name": "cyoa.design",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"format": "prettier --plugin-search-dir . --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest run",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@playwright/test": "^1.37.1",
		"@skeletonlabs/skeleton": "^2.0.0",
		"@skeletonlabs/tw-plugin": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.23.0",
		"@types/node": "^20.5.8",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.6.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"husky": "^8.0.3",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.28",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"prisma": "^5.2.0",
		"prisma-erd-generator-markdown": "^1.3.1",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"typescript-svelte-plugin": "^0.3.30",
		"vite": "^4.4.9",
		"vitest": "^0.34.3",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/poppins": "^5.0.8",
		"typesafe-i18n": "^5.26.2"
	}
}
